{
    "name": "nstapelbroek/culpa",
    "description": "Adds Blameable support to Eloquent models in Laravel 5",
    "keywords": ["laravel", "blameable", "created-by", "updated-by", "deleted-by", "blamable"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ross Masters",
            "email": "ross@rossmasters.com"
        },
        {
            "name": "Nico Stapelbroek",
            "email": "nstapelbroek@gmail.com"
        }
    ],
    "homepage": "https://github.com/nstapelbroek/culpa-laravel-5",
    "support": {
        "issues": "https://github.com/nstapelbroek/culpa-laravel-5/issues"
    },
    "require": {
        "illuminate/support": "~5.1 || ^6.0",
        "illuminate/config": "~5.1 || ^6.0",
        "illuminate/events": "~5.1 || ^6.0",
    },
    "require-dev": {
        "ext-sqlite3": "*",
        "illuminate/database": "~5.1 || ^6.0",
        "phpunit/phpunit": "^5.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Culpa\\": "src/",
            "Culpa\\Tests\\": "tests/"
        }
    },
    "conflict": {
        "rmasters/culpa": "*"
    },
    "config": {
        "preferred-install": "dist"
    }
}
